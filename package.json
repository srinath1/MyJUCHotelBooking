    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-jwt": "^7.7.5",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^6.0.11",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.14",
    "query-string": "^7.1.1",
    "stripe": "^10.3.0",
    "uuid": "^8.3.2"
  }
